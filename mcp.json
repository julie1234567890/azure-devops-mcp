{
    "inputs": [
        {
            "id": "ado_org",
            "type": "promptString",
            "description": "Azure DevOps organization name  (e.g. 'contoso')"
        }
    ],
    "servers": {
        "ado-in-dev": {
            "type": "stdio",
            "command": "node",
            "args": [
                "dist/index.js",
                "${input:ado_org}"
            ]
        }
    }
}